{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.0.0",
  "dependencies": {
    "modernizr": "~2.6.2"<%
    var deps = [];
    if (includeDataTable || includeGallery) deps.push('"jquery": "~1.9.1"');
    if (includeDataTable) deps.push('"datatables": "~1.9.4"');
    if (includeGallery) deps.push('"handlebars.js": "~1.0.10"');
    if (deps.length) print(',\n    ');
    print(deps.join(',\n    '));
    %>
  }
}
